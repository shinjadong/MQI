# Google Sheets 설정
GOOGLE_SERVICE_ACCOUNT_FILE=your-service-account-key.json
GOOGLE_SHEETS_URL=https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID/edit

# Supabase 설정
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Claude AI API 설정
ANTHROPIC_API_KEY=your-anthropic-api-key

# 스케줄 설정
SYNC_INTERVAL_MINUTES=30

# AI 데이터 분류 설정
ENABLE_AI_CLASSIFICATION=true
AI_CLASSIFICATION_TIMEOUT=30

# 알림 설정
ENABLE_KAKAO_NOTIFICATIONS=false
ENABLE_EMAIL_NOTIFICATIONS=false
ENABLE_SLACK_NOTIFICATIONS=true

# 카카오톡 알림톡 설정 (설정 완료 후 활성화)
KAKAO_REST_API_KEY=your-kakao-rest-api-key
KAKAO_SENDER_KEY=your-sender-key
KAKAO_TEMPLATE_CODE_INQUIRY=your-inquiry-template-code
KAKAO_TEMPLATE_CODE_CAREON=your-careon-template-code

# 알림 받을 직원 전화번호 (콤마로 구분)
NOTIFICATION_PHONE_NUMBERS=010-1234-5678,010-9876-5432

# 이메일 알림 설정 (Gmail SMTP)
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
NOTIFICATION_EMAILS=staff1@company.com,staff2@company.com

# 슬랙 알림 설정
SLACK_WEBHOOK_URL=your-slack-webhook-url 